{
  "small_team_test": {
    "id": "small_team_test",
    "name": "Small Team Kitchen Test",
    "description": "Test scenario with a small team handling basic recipes",
    "recipe_ids": ["recipe_1", "recipe_2", "recipe_3"],
    "agent_config": {
      "executive_chefs": 1,
      "sous_chefs": 1,
      "chef_de_parties": 0,
      "line_cooks": 2,
      "prep_cooks": 2,
      "kitchen_porters": 1,
      "team_size": 7,
      "experience_level": "intermediate"
    },
    "kitchen_config": {
      "station_count": 4,
      "equipment_sets": ["basic", "intermediate"],
      "inventory_levels": {
        "high": 100,
        "medium": 50
      },
      "kitchen_layout": "standard",
      "safety_protocols": ["hand_washing", "temperature_control", "cross_contamination"]
    },
    "time_limit": "30m",
    "max_parallel_tasks": 3,
    "quality_thresholds": {
      "min_coordination_score": 0.7,
      "min_coherence_score": 0.7,
      "max_task_failures": 2,
      "min_completion_rate": 0.8,
      "max_time_overrun": 0.2
    },
    "metrics_config": {
      "communication_tracking": true,
      "task_progress_tracking": true,
      "quality_metrics": true,
      "timing_metrics": true,
      "coordination_metrics": false,
      "export_formats": ["csv", "json"]
    }
  },
